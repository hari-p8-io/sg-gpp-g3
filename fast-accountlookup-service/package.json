{
  "name": "fast-accountlookup-service",
  "version": "1.0.0",
  "description": "Account lookup service for PACS message enrichment",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "npx playwright test",
    "test:ui": "npx playwright test --ui",
    "proto:generate": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./src/generated --grpc_out=grpc_js:./src/generated --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` -I ./proto ./proto/*.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.0",
    "uuid": "^9.0.0",
    "moment-timezone": "^0.5.43",
    "@gpp/fast-core": "^1.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "grpc-tools": "^1.12.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "@types/moment-timezone": "^0.5.13",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0"
  },
  "keywords": ["grpc", "account-lookup", "singapore", "pacs", "fintech"],
  "author": "GPP G3 Team",
  "license": "MIT"
} 
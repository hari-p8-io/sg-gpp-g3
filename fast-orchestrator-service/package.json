{
  "name": "fast-orchestrator-service",
  "version": "1.0.0",
  "description": "Fast orchestrator service for payment pipeline orchestration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.11.3",
    "@grpc/proto-loader": "^0.7.13",
    "kafkajs": "^2.2.4",
    "uuid": "^9.0.1",
    "moment-timezone": "^0.5.45",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.1",
    "@types/uuid": "^9.0.8",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/morgan": "^1.9.4",
    "@playwright/test": "^1.47.0",
    "typescript": "^5.5.4",
    "ts-node": "^10.9.2",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3"
  },
  "keywords": ["orchestrator", "kafka", "payment", "pipeline", "microservices"],
  "author": "GPP G3 Team",
  "license": "MIT"
} 